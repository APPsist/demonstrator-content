        <rdfs:label xml:lang="en">department unit</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Organisationseinheit"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Aktion -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Aktion">
        <rdfs:label xml:lang="de">Aktion</rdfs:label>
        <rdfs:label xml:lang="en">action</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Prozesselement"/>
        <terms:description xml:lang="de">Eine auf eine Maschine bezogene Handlung einer Aktivität. Kann aus Schritten zusammengesetzt sein.</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Aktivitaet -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Aktivitaet">
        <rdfs:label xml:lang="de">Aktivität</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Prozesselement"/>
        <terms:description xml:lang="de">Eine Aktivität beschreibt allgemeine, nicht auf konkrete Maschine oder Werkstoffe bezogene Handlungsabläufe, die aus einer Folge von Schritten (Aktionen oder aus Aktivitäten) bestehen.</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Anlage -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Anlage">
        <rdfs:label xml:lang="de">Anlage</rdfs:label>
        <rdfs:label xml:lang="en">installation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Produktionsmittel"/>
        <terms:description xml:lang="de">Stationär betriebenes Arbeitsmittel, das einen kompletten Produktionsprozess realisiert.</terms:description>
        <terms:description xml:lang="en">Stationary device that implements a complete production process.</terms:description>
    </owl:Class>
    



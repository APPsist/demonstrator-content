    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Anlagenbediener -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Anlagenbediener">
        <rdfs:label xml:lang="de">Anlagenbediener</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Ausfuehrende"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/AnlagenbedingteStoerung -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/AnlagenbedingteStoerung">
        <rdfs:label xml:lang="de">Anlagenbedingte Störung</rdfs:label>
        <rdfs:label xml:lang="en">internal disabled state</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Stoerung"/>
        <terms:description xml:lang="de">Zustand  einer  Einheit, gekennzeichnet durch eine Unfähigkeit, eine geforderte Funktion auszuführen.</terms:description>
        <terms:description xml:lang="en">State of a machine characterized either by an  inability  to  perform  a  required function.</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Anlagenfuehrer -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Anlagenfuehrer">
        <rdfs:label xml:lang="de">Anlagenführer</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Ausfuehrende"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Anlagenoperator -->


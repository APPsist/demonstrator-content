        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Funktion"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Ausfuehrende -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Ausfuehrende">
        <rdfs:label xml:lang="de">Ausführende</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Stelle"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Baugruppe -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Baugruppe">
        <rdfs:label xml:lang="de">Baugruppe</rdfs:label>
        <rdfs:label xml:lang="en">subassembly</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Erzeugnisse"/>
        <terms:description xml:lang="de">Bezeichnung für eine Gruppe von Einzelteilen, die als Einheit in das Endprodukt eingeht. Viele Industrieprodukte bestehen aus mehreren Einzelteilen, die durch Montagearbeitsgänge zu Baugruppen und schließlich zu Endprodukten zusammengefügt werden (Gabler Wirtschaftslexikon).</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Bearbeitung -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Bearbeitung">
        <rdfs:label xml:lang="de">Bearbeitung</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Aufgabe"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Bedienung -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Bedienung">
        <rdfs:label xml:lang="de">Bedienung</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Aufgabe"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Beispiel -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Beispiel">
        <rdfs:label xml:lang="de">Beispiel</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Erlaeuterung"/>
        <terms:description xml:lang="de">Einzelfall, der zufällig aus einer Menge genommen wurde, der als Erklärung oder Nachweis dient.</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Beschaeftigter -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Beschaeftigter">
        <rdfs:label xml:lang="de">Beschäftigter</rdfs:label>
        <rdfs:label xml:lang="en">employee</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Organisationseinheit"/>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Betriebsmittel -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Betriebsmittel">
        <rdfs:label xml:lang="de">Betriebsmittel</rdfs:label>
        <rdfs:label xml:lang="en">equipment</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Produktionsgegenstand"/>
        <skos:note xml:lang="de">Quelle: Skript der Vorlesung &quot;Grundlagen der Betriebswirtschaftslehre&quot; von Prof. Hentze.</skos:note>
        <terms:description xml:lang="de">Zu den Betriebsmitteln gehört die gesamte technische Apparatur, deren sich der Betrieb zur Durchführung des Betriebsprozesses bedient, wie Maschinen, maschinelle Anlagen und Werkzeuge aber auch Grundstücke, Gebäude, Verkehrmittel, Transport- und Büroeinrichtungen. Sie werden im Gegensatz zu den Werkstoffen nicht verbraucht.</terms:description>
        <terms:description xml:lang="en">Equipment includes the entire technical apparatus used during production as well as property (plot of land), buildings, etc. In contrast to materials, equipment is not consumed.</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Betriebsstoff -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Betriebsstoff">
        <rdfs:label xml:lang="de">Betriebsstoff</rdfs:label>
        <rdfs:label xml:lang="en">consumable</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/Werkstoff"/>
        <skos:note xml:lang="de">Quelle: Skript der Vorlesung &quot;Grundlagen der Betriebswirtschaftslehre&quot; von Prof. Hentze.</skos:note>
        <terms:description xml:lang="de">Stoffe, die nicht unmittelbar in das Fertigungsprodukt eingehen, sondern im Fertigungsprozess verbraucht werden, bzw. diesen ermöglichen (z.B.: Energie Schmiermittel, Reinigungsmaterial, Büromaterial).</terms:description>
        <terms:description xml:lang="en">Indirect materials not incorporated into the final product, but consumed during the production process or enabling it (e.g., lubricant, oils, etc consumed in a typical manufacturing facility).</terms:description>
    </owl:Class>
    


    <!-- http://www.appsist.de/ontology/Betriebszustand -->

    <owl:Class rdf:about="http://www.appsist.de/ontology/Betriebszustand">
        <rdfs:label xml:lang="de">Betriebszustand</rdfs:label>
        <rdfs:label xml:lang="en">operating state</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.appsist.de/ontology/FunktionsfaehigerZustand"/>
        <terms:description xml:lang="de">Zustand,  in  dem  eine  Einheit  die  geforderte Funktion erbringt.</terms:description>
        <terms:description xml:lang="en">State when an item is performing as required.</terms:description>
    </owl:Class>
    


